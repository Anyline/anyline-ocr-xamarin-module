<?xml version="1.0"?>
	<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
		<metadata>
			<!-- The identifier that must be unique within the hosting gallery -->
			<id>Anyline.Xamarin.SDK.Droid</id>

			<!-- The package version number that is used when resolving dependencies -->
			<version>3.22.0</version>

			<!-- Authors contain text that appears directly on the gallery -->
			<authors>Anyline GmbH</authors>

			<!-- Owners are typically nuget.org identities that allow gallery
			users to earily find other packages by the same owners.  -->
			<owners>peteranyline</owners>

			<!-- License and project URLs provide links for the gallery -->
			<licenseUrl>https://raw.githubusercontent.com/Anyline/anyline-ocr-xamarin-module/master/LICENSE.md</licenseUrl>
			<!--<projectUrl>GITHUB REPO</projectUrl>-->

			<!-- The icon is used in Visual Studio's package manager UI -->
			<iconUrl>https://anylinewebsiteresource.blob.core.windows.net/wordpressmedia/2015/11/logo-300x138.png</iconUrl>

			<!-- If true, this value prompts the user to accept the license when
			installing the package. -->
			<requireLicenseAcceptance>true</requireLicenseAcceptance>

			<!-- Any details about this particular release -->
			<releaseNotes>https://documentation.anyline.com/toc/platforms/xamarin/release_guide/index.html</releaseNotes>

			<!-- The summary -->
			<summary>An easy-to-use SDK for applications to enable Optical Character Recognition (OCR) on mobile devices.</summary>
			
			<!-- The description can be used in package manager UI. Note that the
			nuget.org gallery uses information you add in the portal. -->
			<description>
			Anyline provides an easy-to-use SDK for applications to enable Optical Character Recognition (OCR) on mobile devices.
			
			This NuGet package contains the Xamarin.Android SDK binary.
			
			For detailed integration guides, see https://documentation.anyline.com/toc/platforms/xamarin/xamarin.ios/xamarin.ios_quickstart.html and https://documentation.anyline.com/toc/platforms/xamarin/xamarin.android/xamarin.android_quickstart.html 
			</description>

			<projectUrl>https://documentation.anyline.io/toc/platforms/xamarin/index.html</projectUrl>
			
			<!-- Copyright information -->
			<copyright>Copyright Â© 2018 Anyline GmbH</copyright>

			<!-- Tags appear in the gallery and can be used for tag searches -->
			<tags>ocr scanning meter barcode mrz qr document camera anyline license plate </tags>
						
		</metadata>

		<files>
			<!-- A readme.txt will be displayed when the package is installed -->
			<file src="..\README.md" target="" />
		
			<!-- SDK .dll -->
			<file src="..\AnylineXamarinSDK.Droid.dll" target="lib\MonoAndroid10" />
			
		</files>
				
    </package>