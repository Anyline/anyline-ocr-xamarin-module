{
    "captureResolution":"1080",
    "cutout": {
        "style": "rect",
        "maxWidthPercent": "80%",
        "maxHeightPercent": "80%",
        "alignment": "center",
        "width": 450,
        "ratioFromSize": {
            "width": 1,
            "height": 1
        },
        "strokeWidth": 2,
        "cornerRadius": 320,
        "strokeColor": "FFFFFF",
        "outerColor": "000000",
        "outerAlpha": 0.3
    },
    "flash": {
        "mode": "manual",
        "alignment": "bottom_right"
    },
    "visualFeedback": {
        "style": "rect",
        "strokeWidth": 3,
        "strokeColor": "0099FF",
        "cornerRadius": 10
        
    },
    "beepOnResult": true,
    "vibrateOnResult": true,
    "blinkAnimationOnResult": true, 
    "cancelOnResult": false 
}