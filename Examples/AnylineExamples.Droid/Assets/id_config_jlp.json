{
  "camera": {
    "captureResolution": "1080p",
    "zoomGesture": true
  },
  "flash": {
    "mode": "manual",
    "alignment": "bottom_right"
  },
  "viewPlugin" : {
    "plugin": {
      "id": "ID",
      "idPlugin": {
        "japaneseLandingPermissionConfig": {}
      }
    },
    "cutoutConfig": {
      "style": "rect",
      "maxWidthPercent": "70%",
      "maxHeightPercent": "70%",
      "alignment": "center",
      "strokeWidth": 2,
      "cornerRadius": 4,
      "strokeColor": "FFFFFF",
      "outerColor": "000000",
      "outerAlpha": 0.3,
      "ratioFromSize": {
        "width": 440,
        "height": 615
      },
      "feedbackStrokeColor": "0099FF"
    },
    "scanFeedback": {
      "style": "CONTOUR_RECT",
      "visualFeedbackRedrawTimeout": 100,
      "strokeColor": "0099FF",
      "fillColor": "220099FF",
      "beepOnResult": true,
      "vibrateOnResult": true,
      "strokeWidth": 2
    },
    "cancelOnResult": true
  }
}