{
  "camera": {
    "captureResolution": "720"
  },
  "flash": {
    "mode": "manual",
    "alignment": "bottom_left"
  },
  "viewPlugin": {
    "plugin": {
      "id": "TIN_ID",
      "ocrPlugin": {
        "tinConfig": {
        }
      }
    },
    "cutoutConfig": {
      "style": "rect",
      "width": 720,
      "alignment": "center",
      "maxWidthPercent": "80%",
      "ratioFromSize": {
        "width": 720,
        "height": 144
      },
      "strokeWidth": 2,
      "strokeColor": "FFFFFF",
      "cornerRadius": 4,
      "outerColor": "000000",
      "outerAlpha": 0.5,
      "feedbackStrokeColor": "0099FF",
      "offset": {
        "x": 0,
        "y": -15
      }
    },
    "scanFeedback": {
      "style": "CONTOUR_RECT",
      "strokeColor": "0099FF",
      "fillColor": "220099FF",
      "beepOnResult": true,
      "vibrateOnResult": true,
      "blinkAnimationOnResult": true
    },
    "cancelOnResult": true,
    "delayStartScanTime": 2000
  }
}