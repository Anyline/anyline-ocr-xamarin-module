{
  "camera": {
    "captureResolution": "1080p"
  },
  "flash": {
    "mode": "manual",
    "alignment": "bottom_right"
  },
  "viewPlugin": {
    "plugin": {
      "id": "Container_ID",
      "ocrPlugin": {
        "containerConfig": {
          "scanMode": "vertical"
        }
      }
    },
    "cutoutConfig": {
      "style": "rect",
      "maxWidthPercent": "10%",
      "alignment": "top_half",
      "ratioFromSize": {
        "width": 9,
        "height": 62
      },
      "strokeWidth": 2,
      "strokeColor": "FFFFFF",
      "cornerRadius": 4,
      "outerColor": "000000",
      "outerAlpha": 0.5,
      "feedbackStrokeColor": "0099FF",
      "offset": {
        "x": 0,
        "y": -15
      }
    },
    "scanFeedback": {
      "style": "CONTOUR_RECT",
      "strokeColor": "0099FF",
      "fillColor": "220099FF",
      "blinkOnResult": true,
      "beepOnResult": true,
      "vibrateOnResult": true
    },
    "cancelOnResult": true
  }
}