{
  "captureResolution": "1080p",

  "cutout": {
    "style": "image",
    "image": "cutout_image",
    "maxWidthPercent": "100%",
    "maxHeightPercent": "80%",
    "alignment": "top_half",
    "ratioFromSize": {
      "width": 125,
      "height": 85
    },
    "strokeWidth": 0,
    "cornerRadius": 0,
    "strokeColor": "FFFFFF",
    "feedbackStrokeColor": "FFFFFF",
    "outerColor": "7F7F7F",
    "outerAlpha": 0.7,
    "cropPadding": {
      "x": 0,
      "y": 0
    },
    "cropOffset": {
      "x": 0,
      "y": 0
    },
    "offset": {
      "x": 0,
      "y": 30
    }
  },
  "flash": {
    "mode": "manual",
    "alignment": "bottom_left"
  },
  "beepOnResult": true,
  "vibrateOnResult": true,
  "blinkAnimationOnResult": true,
  "cancelOnResult": false,
  "visualFeedback": {
    "style": "rect",
    "strokeColor": "0099FF",
    "strokeWidth": 2
  }
}