{
    "captureResolution": "720p",

    "cutout": {
        "style": "rect",
        "maxWidthPercent": "80%",
        "alignment": "top_half",
        "ratioFromSize": {
            "width": 100,
            "height": 80
        },
        "strokeWidth": 4,
        "cornerRadius": 10,
        "strokeColor": "000000",
        "outerColor": "000000",
        "outerAlpha": 0.3,
        "feedbackStrokeColor": "000066"
    },
    "flash": {
        "mode": "manual",
        "alignment": "bottom_right"
    },
    "beepOnResult": true,
    "vibrateOnResult": true,
    "blinkAnimationOnResult": true,
    "cancelOnResult": false,
    "visualFeedback": {
        "style": "rect",
        "strokeColor": "007ACC",
        "fillColor": "22007ACC",
        "animationDuration": 150
    }
}