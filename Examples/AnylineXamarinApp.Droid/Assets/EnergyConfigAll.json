{
    "captureResolution": "720p",
  "cutout": {
    "style": "rect",
    "width": 540,
    "maxWidthPercent": "80%",
    "maxHeightPercent": "80%",
    "alignment": "top_half",
    "image": "Overlay",
    "ratioFromSize": {
      "width": 125,
      "height": 85
    },
    "strokeWidth": 2,
    "strokeColor": "FFFFFF",
    "cornerRadius": 4,
    "offset": {
      "x": 0,
      "y": 0
    },
    "cropPadding": {
      "x": 0,
      "y": 0
    },
    "cropOffset": {
      "x": 0,
      "y": 0
    },
    "outerColor": "000000",
    "outerAlpha": 0.3
    },
    "flash": {
    "mode": "auto",
    "alignment": "bottom_left",
    "imageOn": "ic_flash_on",
    "imageOff": "ic_flash_off",
    "imageAuto": "ic_flash_auto"
    },
    "beepOnResult": false,
    "vibrateOnResult": true,
    "blinkAnimationOnResult": true,
    "cancelOnResult": true,
