{
    "captureResolution": "720",
    "cutout": {
        "style": "rect",
        "maxWidthPercent": "80%",
        "maxHeightPercent": "80%",
        "alignment": "center",
        "width": 300,
        "ratioFromSize": {
            "width": 1,
            "height": 1
        },
        "strokeWidth": 2,
        "cornerRadius": 300,
        "strokeColor": "FFFFFF",
        "outerColor": "000000",
        "outerAlpha": 0.3,
        "offset": {
            "x": 0,
            "y": 0
        },
        "cropOffset": {
            "x": 0,
            "y": 0
        },
        "cropPadding": {
            "x": 10,
            "y": 10
        }
    },
    "flash": {
        "mode": "manual",
        "alignment": "bottom_right"
    },
    "beepOnResult": true,
    "vibrateOnResult": true,
    "blinkAnimationOnResult": true,
    "cancelOnResult": true,
    "visualFeedback": {
        "style": "rect",
        "strokeWidth": 3,
        "strokeColor": "8800FF00",
        "fillColor": "3300FF00",
        "cornerRadius": 10
    }
}