{
  "captureResolution":"720p",

  "cutout": {
    "style": "rect",
    "width":  300,
    "maxWidthPercent": "80%",
    "alignment": "center",
    "ratioFromSize": {
      "width": 1,
      "height": 1
    },
    "strokeWidth": 4,
    "cornerRadius": 10,
    "strokeColor": "000000",
    "outerColor": "000000",
    "outerAlpha": 0.3,
    "feedbackStrokeColor": "0099FF"
  },
  "flash": {
    "mode": "manual",
    "alignment": "bottom_right"
  },
  "beepOnResult": true,
  "vibrateOnResult": true,
  "blinkAnimationOnResult": true,
  "cancelOnResult": false,
  "visualFeedback": {
    "style": "rect",
    "strokeColor": "0099FF",
    "fillColor": "220099FF",
    "animationDuration": 150
  }
}