{
  "camera": {
    "captureResolution": "1080"
  },
  "flash": {
    "mode": "manual",
    "alignment": "bottom_right"
  },
  "viewPlugin": {
    "plugin": {
      "id": "OCR_BOTTLECAP_PLUGIN",
      "ocrPlugin": {
        "ocrConfig": {
          "scanMode": "GRID",
          "languages": [ "bottlecap.traineddata" ],
          "charWhitelist": "123456789ABCDEFGHJKLMNPRSTUVWXYZ",
          "minCharHeight": 14,
          "maxCharHeight": 95,
          "minConfidence": 75,
          "validationRegex": "^[0-9A-Z]{3}\n[0-9A-Z]{3}\n[0-9A-Z]{3}",
          "charCountX": 3,
          "charCountY": 3,
          "charPaddingXFactor": 0.3,
          "charPaddingYFactor": 0.5,
          "isBrightTextOnDark": true
        }
      }
    },
    "cutoutConfig": {
      "style": "rect",
      "maxWidthPercent": "80%",
      "maxHeightPercent": "80%",
      "alignment": "center",
      "width": 450,
      "ratioFromSize": {
        "width": 1,
        "height": 1
      },
      "strokeWidth": 2,
      "cornerRadius": 300,
      "strokeColor": "FFFFFF",
      "outerColor": "000000",
      "outerAlpha": 0.3,
      "cropPadding": {
        "x": 10,
        "y": 10
      },
      "feedbackStrokeColor": "0099FF"
    },
    "scanFeedback": {
      "style": "rect",
      "strokeWidth": 1,
      "strokeColor": "0099FF",
      "fillColor": "220099FF",
      "cornerRadius": 10,
      "beepOnResult": true,
      "vibrateOnResult": true,
      "blinkAnimationOnResult": true
    },
    "cancelOnResult": true
  }
}