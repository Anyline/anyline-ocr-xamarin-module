{
  "camera": {
    "captureResolution": "1080p"
  },
  "flash": {
    "mode": "auto",
    "alignment": "bottom_right"
  },
  "viewPlugin": {
    "plugin": {
      "id": "Barcode_ID",
      "barcodePlugin": {
        "barcodeFormatOptions": [ "ALL" ]
      }
    },
    "cutoutConfig": {
      "style": "rect",
      "maxWidthPercent": "80%",
      "maxHeightPercent": "80%",
      "alignment": "center",
      "ratioFromSize": {
        "width": 100,
        "height": 80
      },
      "strokeWidth": 1,
      "cornerRadius": 3,
      "strokeColor": "FFFFFF",
      "outerColor": "000000",
      "outerAlpha": 0.3,
      "feedbackStrokeColor": "0099FF"
    },
    "scanFeedback": {
      "style": "rect",
      "strokeColor": "0099FF",
      "fillColor": "220099FF",
      "animationDuration": 150,
      "blinkOnResult": true,
      "beepOnResult": true,
      "vibrateOnResult": true
    },
    "cancelOnResult": true
  }
}